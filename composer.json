{
  "name": "bitbirddev/oh-dear-bundle",
  "description": "A simple Symfony bundle to integrate OhDear Application Health monitoring",
  "type": "symfony-bundle",
  "require": {
    "php": "^8.1",
    "ohdearapp/health-check-results": "^1.0",
    "symfony/framework-bundle": "^6.0 | ^7.0",
    "spatie/cpu-load-health-check": "^1.0",
    "spatie/regex": "^3.1",
    "symfony/http-client": "^6.0 | ^7.0",
    "symfony/console": "^6.0 | ^7.0",
    "nesbot/carbon": "^2.72",
    "symfony/messenger": "^6.0 | ^7.0",
    "symfony/scheduler": "^6.0 | ^7.0",
    "composer/semver": "^3.4"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "bitbirddev\\OhDearBundle\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Benjamin Schneider",
      "email": "bs@bitbird.de"
    }
  ],
  "require-dev": {
    "phpro/grumphp-shim": "^2.2",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.38",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpunit/phpunit": "^9.0",
    "phpunit/php-code-coverage": "^9.0",
    "symfony/phpunit-bridge": "^6.3",
    "doctrine/orm": "^2.16",
    "pimcore/pimcore": "^11"
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp-shim": true
    }
  }
}
